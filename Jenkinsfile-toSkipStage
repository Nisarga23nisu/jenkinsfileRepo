parallel {
   
    agent { label 'slave' } 
     parameters {
     booleanParam defaultValue: true, description: 'Run the Test stage?', name: 'RUN_TEST'
}

stages{
 
    when {
        expression { params.RUN_TEST }
    }
     stage('BUILD') {
     steps{
        echo "EXECUTING BUILDS "
     }
  }

stage ('TEST'){
    steps{
        echo "EXECUTING TEST "
    }
}

}



}
