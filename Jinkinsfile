pipeline{
  agent { label 'slave-1'}
  stages{
    stage("Build stages") {
        steps{
           sh '''
               echo "cd /var/lib/jenkins/
                ls -ll 
               '''
              }
              } 
    stage("test stages") {
         steps{
           sh '''
            mkdir dir1 dir2
            chmod -R 777 dir1 dir2
            '''
               }
      }
}
}












}
